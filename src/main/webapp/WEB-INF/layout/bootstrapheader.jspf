
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>

<c:set var="bootstrapbase" value="//maxcdn.bootstrapcdn.com/bootstrap/4.5.0"/>   
<%--! webjar CSS/JS dependencies --%>
<c:url var="bootstrapbase" value="/webjars/bootstrap/4.5.0"/>   
<c:url var="jquerybase" value="/webjars/jquery/3.5.1"/>   

<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1">

<!-- Latest compiled and minified CSS -->
<link rel="stylesheet" href="${bootstrapbase}/css/bootstrap.min.css" >

<!-- Optional theme -->
<link rel="stylesheet" href="${bootstrapbase}/css/bootstrap-theme.min.css" >

<!-- Latest compiled and minified JavaScript -->
<script src="${jquerybase}/jquery.min.js"> </script>

<script src="${bootstrapbase}/js/bootstrap.min.js"> </script>
